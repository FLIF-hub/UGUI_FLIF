{
  "name": "ugui_flif",
  "main": "index.htm",
  "version": "0.3.0",
  "author": "The Jared Wilcurt",
  "description": "Free Lossless Image Format",
  "keywords": [
    "image",
    "compression",
    "lossless"
  ],
  "window": {
    "title": "UGUI: FLIF",
    "icon": "_img/icon32.png",
    "toolbar": false,
    "resizable": true,
    "visible": true,
